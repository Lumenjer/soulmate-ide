  "alias": {
    "@auth0/auth0-spa-js": "./node_modules/@auth0/auth0-spa-js/dist/lib/auth0-spa-js.cjs.js",
    "react-dom": "@hot-loader/react-dom"
  },
  "scripts": {
    "react-start": "parcel -p 3000 index.html",
    "react-build": "yarn build-worker && parcel build index.html --out-dir build --public-url / --no-source-maps",
    "react-build-desktop": "yarn build-worker && parcel build index.html --out-dir build --public-url . --no-source-maps",
    "start-servers": "concurrently \"yarn --cwd services/build-server start\" \"docker run -p 8080:8080 -d elliottkember/wokwi-hexi\"",
    "start": "yarn && concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron . \" \"yarn build-worker-dev\"",
    "start-local": "yarn && concurrently \"yarn start-servers\" \"yarn start-app-local\"",
    "start-app-local": "yarn && concurrently \"cross-env local=true BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron . \" \"yarn build-worker-dev\"",
    "electron-build": "electron-builder",
    "build": "yarn react-build-desktop && yarn electron-build",
    "build-worker": "parcel build node_modules/monaco-editor/esm/vs/editor/editor.worker.js -d build --no-source-maps",
    "build-worker-dev": "parcel build node_modules/monaco-editor/esm/vs/editor/editor.worker.js --no-source-maps",
    "release": "yarn clean && yarn version --patch && yarn react-build-desktop && AWS_PROFILE=soulmate-ide-updates electron-builder -mw --publish always",
    "clean": "rm -rf ./build ./.cache ./dist",
    "postinstall": "npx electron-builder install-app-deps",
    "install-dependencies": "git submodule update --init --recursive; (yarn --cwd services/build-server); (cd services/wokwi-server && npm install)",
    "web": "NODE_ENV=development parcel index.html --open -p 3000",
    "bump": "yarn version --patch && git push && git push --tags"
  },
  "build": {
    "afterSign": "electron-builder-notarize",
    "win": {
      "publish": {
        "provider": "s3",
        "bucket": "soulmate-ide-updates"
      }
    },
    "mac": {
      "publish": {
        "provider": "s3",
        "bucket": "soulmate-ide-updates"
      },
      "entitlements": "entitlements.plist",
      "entitlementsInherit": "entitlements.plist",
      "gatekeeperAssess": false,
      "hardenedRuntime": true
    },
    "dmg": {
      "sign": false
    },
    "directories": {
      "buildResources": "assets"
    },
    "appId": "com.elliottkember.soulmate",
    "files": [
      "src/icon.icns",
      "src/icon.png",
      "src/electron.js",
      "src/preload.js",
      "src/auth.js",
      "build/**/*",
      "package.json"
    ],
    "extraFiles": [
      "builder"
    ]
  }
}
