{
  "name": "Soulmate",
  "version": "1.0.0",
  "main": "src/electron.js",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-react": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "avr8js": "^0.9.1",
    "bonjour": "^3.5.0",
    "electron-is-dev": "^1.2.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": ">=15",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-icons": "^3.10.0",
    "react-monaco-editor": "^0.36.0",
    "react-router-dom": "^5.2.0",
    "semantic-ui-css": "^2.4.1",
    "typescript": "^3.9.3",
    "use-light-switch": "^1.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^9.0.0",
    "electron-builder": "^22.7.0",
    "parcel-bundler": "^1.12.4",
    "wait-on": "^5.0.0"
  },
  "scripts": {
    "react-start": "parcel -p 3000 index.html",
    "react-build": "parcel build index.html --out-dir build --public-url ./",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron . \"",
    "electron-build": "electron-builder ",
    "build": "yarn react-build && yarn electron-build --icon src/AppIcon.icns"
  },
  "build": {
    "appId": "com.soulmatelights.editor",
    "files": [
      "src/icon.png",
      "src/electron.js",
      "src/preload.js",
      "build/**/*"
    ]
  }
}
